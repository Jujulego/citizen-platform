// Popup de Modif Citoyen
div(class="modal fade" id="modifAsso" tabindex="-1" role="dialog")
    div(class="modal-dialog modal-dialog-centered" role="document")
        form(class="modal-content" method="post" action="/asso/modifAsso")
            div(class="modal-header")
                h5(class="modal-title")
                    span   Modifier le Profil de mon Association
                button(class="close" type="button" data-dismiss="modal")
                    i(class="fas fa-times")
            
            div(class="modal-body") 
                div(class="form-group")
                    label(for="inputNom") Nom :
                    input(id="inputNom" name="nom" class="form-control" type="text" value=asso.nom)
                div(class="form-group")
                    label(for="inputAdresse") Address :
                    input(id="inputAdresse" name="adresse" class="form-control" type="text" value=asso.adresse)
                div(class="form-group")
                    label(for="inputTel") Tel : 
                    input(id="inputTel" name="tel" class="form-control" type="text" value=asso.tel)
                div(class="form-group")
                    label(for="inputSiteWeb") Site Web :
                    input(id="inputSiteWeb" name="siteWeb" class="form-control" type="text" value=asso.siteWeb)
                div(class="form-group")
                    label(for="inputSiret") Siret :
                    input(id="inputSiret" name="siret" class="form-control" type="text" value=asso.siret)
                
            div(class="modal-footer")
                button(class="btn btn-primary" type="submit") OK
